<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lksnext.parkingagarcia">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lks_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/lks_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParkingLKS">
        <activity
            android:name="com.lksnext.parkingagarcia.view.activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.lksnext.parkingagarcia.view.activity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lksnext.parkingagarcia.view.activity.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.lksnext.parkingagarcia.view.activity.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.lksnext.parkingagarcia.view.activity.PasswordUpdatedActivity"
            android:exported="false" />
    </application>

</manifest>